{
  "version": "0.2.0",
  "configurations": [
    {
        "name": "test_debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/libvroom-example/libvroom-example",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/libvroom-example",
        "preLaunchTask": "make build",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          },
          {
            "description": "Set Disassembly Flavor to Intel",
            "text": "-gdb-set disassembly-flavor intel",
            "ignoreFailures": true
          }
        ]
      }
  ]
}